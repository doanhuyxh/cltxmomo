            <script>


                // Function để cập nhật dữ liệu trong bảng
                function updateTableData() {

                    fetch("/Home/HistoryWin")
                        .then(res => res.json())
                        .then(res => {
                            var currentDataSetIndex = 0;

                            var tableBody = document.getElementById("history");
                            tableBody.innerHTML = ""; // Xóa dữ liệu hiện tại trong bảng

                            // Duyệt qua mảng dữ liệu của lần hiện tại và thêm dữ liệu vào bảng
                            res.forEach(function (data) {
                                var row = "<tr>" +
                                    "<td>" + data.player + "</td>" +
                                    "<td>" + data.bet + "</td>" +
                                    "<td>" + data.win + "</td>" +
                                    "<td><span class='label label-info'>" + data.content + "</span></td>" +
                                    "<td><span class='label label-secondary text-uppercase'>" + data.status + "</span></td>" +
                                    "</tr>";
                                tableBody.innerHTML += row;
                            });
                        })


                }

                // Function để cập nhật countdown timer
                function updateCountdown() {
                    var countdownElement = document.querySelector('.countdown-time');
                    var seconds = parseInt(countdownElement.innerText);

                    if (seconds > 0) {
                        seconds--;
                    } else {
                        seconds = 30;
                        updateTableData(); // Gọi hàm cập nhật dữ liệu trong bảng khi số đếm về 0
                    }

                    countdownElement.innerText = seconds;
                }

                // Cập nhật countdown mỗi giây
                setInterval(updateCountdown, 1000);

                // Cập nhật dữ liệu ban đầu
                updateTableData();
            </script>
